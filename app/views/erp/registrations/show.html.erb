<div class="panel panel-primary">
  <div class="panel-heading">
    <span class="pull-right"><a type="btn" class="btn btn-primary btn-xs print-btn"><i class="fa fa-lg fa-print"></i></a></span>
    <div class="panel-title"><%= @reg.code.code == '0' ? '入库' : '出库' %>清单</div>
  </div>
  <div class="panel-body">
    <div id="toprint">
      <div class="row">
        <div class="col-xs-3 col-sm-3 col-md-3 col-lg-3"></div>
        <div class="col-xs-6 col-sm-6 col-md-6 col-lg-6">清单</div>
        <div class="col-xs-3 col-sm-3 col-md-3 col-lg-3"></div>
      </div>
    </div>
  </div>
</div>

<script type="text/javascript">
  $(".print-btn").click(function(){
  	console.log($("#toprint")[0])
    print_element($("#toprint")[0]);
  });
</script>